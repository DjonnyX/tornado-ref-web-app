{
  "name": "tornado-ref-web-app",
  "version": "1.4.4",
  "author": {
    "email": "djonnyx@gmail.com",
    "name": "Evgeny Grebennikov"
  },
  "description": "PA & AP systems for TornadoSST",
  "scripts": {
    "ng": "ng",
    "start": "npm run serve:cms",
    "serve:cms": "ng serve --port 4201 --configuration cms",
    "serve:admin": "ng serve --port 4201 --configuration admin",
    "serve:documentation": "ng serve --port 4201 --configuration documentation",
    "build": "npm run build:cms && npm run build:admin",
    "build:cms": "ng build --configuration cms --output-path=dist/tornado-cms",
    "build:admin": "ng build --configuration admin --output-path=dist/tornado-admin",
    "build:documentation": "ng build --configuration documentation --output-path=dist/tornado-documentation",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "docker:build": "docker build -t djonnyx/tornadosst-ref-web-app .",
    "docker:run": "docker run -p 3000:80 djonnyx/tornadosst-ref-web-app",
    "docker:push": "docker push djonnyx/tornadosst-ref-web-app"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.1.2",
    "@angular/cdk": "^11.2.13",
    "@angular/common": "^11.1.2",
    "@angular/compiler": "^11.1.2",
    "@angular/core": "^11.1.2",
    "@angular/flex-layout": "^11.0.0-beta.33",
    "@angular/forms": "^11.1.2",
    "@angular/localize": "^11.2.14",
    "@angular/material": "^11.2.13",
    "@angular/material-moment-adapter": "^11.2.13",
    "@angular/platform-browser": "^11.1.2",
    "@angular/platform-browser-dynamic": "^11.1.2",
    "@angular/router": "^11.1.2",
    "@djonnyx/tornado-types": "^1.6.25",
    "@ngrx/effects": "^10.1.2",
    "@ngrx/entity": "^10.1.2",
    "@ngrx/store": "^10.1.2",
    "@ngrx/store-devtools": "^10.1.2",
    "color": "^3.2.1",
    "material-design-icons": "^3.0.1",
    "moment": "^2.29.1",
    "ngrx-store-localstorage": "^10.1.2",
    "ngx-color-picker": "^11.0.0",
    "rxjs": "^6.6.7",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1101.4",
    "@angular/cli": "^11.1.4",
    "@angular/compiler-cli": "^11.1.2",
    "@angular/language-service": "^11.1.2",
    "@types/color": "^3.0.2",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "^2.0.10",
    "@types/lodash": "^4.14.176",
    "@types/node": "^12.20.28",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~5.2.3",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~3.0.1",
    "karma-jasmine-html-reporter": "^1.7.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "^4.1.6"
  }
}
